<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Azure Inventory</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="navbar navbar-fixed-top">
        <div class="container">
            <a href="#" class="pull-left"><img style="width:40%; height:40%;" src="~/Images/ITA_logo_transparent.png"></a>
            <a href="#" class="pull-right"><img style="width:68px; height:65px;" src="~/Images/citySeal.gif"></a>
        </div>
    </div>
    <div class="container body-content" style="padding-top:15px">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year dina903</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
